{
  "manifest_version": 2,
  "name": "Search Logger",
  "version": "1.0",
  "description": "Logs search queries from Google and other search engines.",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "downloads",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icons/icon.png"
  }
}
